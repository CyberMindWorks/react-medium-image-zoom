    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.16.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">= 16.8"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.4.5",
