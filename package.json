{
  "name": "react-medium-image-zoom",
  "version": "4.0.0",
  "description": "Accessible medium.com-style image zoom for React",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:rpearce/react-medium-image-zoom.git"
  },
  "homepage": "https://github.com/rpearce/react-medium-image-zoom",
  "bugs": "https://github.com/rpearce/react-medium-image-zoom/issues",
  "author": "Robert Pearce <me@robertwpearce.com>",
  "license": "BSD-3",
  "keywords": [
    "react",
    "medium",
    "modal",
    "zoom",
    "image",
    "react-component"
  ],
  "tags": [
    "react",
    "medium",
    "modal",
    "zoom",
    "image",
    "react-component"
  ],
  "sideEffects": false,
  "files": [
    "AUTHORS",
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && run-p build:js build:docs",
    "build:docs": "mkdir -p docs && build-storybook -o docs",
    "build:js": "rollup -c ./rollup.config.js",
    "build:js:watch": "npm run build:js -- -w",
    "clean": "run-p clean:dist clean:docs",
    "clean:dist": "rm -rf ./dist",
    "clean:docs": "rm -rf ./docs",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "coverage": "npm run test --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "prepublishOnly": "run-p lint test && npm run build:js",
    "start": "run-p build:js:watch storybook",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.2.0",
    "all-contributors-cli": "^6.9.1",
    "autoprefixer": "^9.6.1",
    "axe-core": "^3.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.6",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.0",
    "eslint-plugin-react-hooks": "2.1.1",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "rollup": "^1.22.0",
    "rollup-plugin-auto-external": "^3.0.0-alpha.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "tinygen": "^0.2.0"
  }
}
